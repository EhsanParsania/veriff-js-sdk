{
  "name": "@veriff/js-sdk",
  "version": "1.0.0",
  "description": "JavaScript SDK for Veriff identity verification",
  "main": "lib/veriff.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js --mode production",
    "dev": "webpack --progress --colors --watch --config webpack/webpack.config.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "Maksim Tsvetkov <maksim.tsvetkov@veriff.com> (https://github.com/MaxFlower)",
  "keywords": [
    "veriff",
    "sdk",
    "online",
    "identity",
    "verification"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.7.1",
    "@types/jest": "^24.0.23",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "jasmine-core": "^2.99.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "sinon": "^4.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yargs": "^11.1.0"
  },
  "dependencies": {}
}
